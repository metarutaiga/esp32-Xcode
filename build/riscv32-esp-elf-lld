#!/bin/bash

ARGS=""
while [[ $# > 0 ]] ; do
    case $1 in
        -dead_strip*)
        ;;
        -F*)
        ;;
        -filelist*)
        while read -r line; do
            ARGS=$ARGS" "$line
        done < $2
        shift ;;
        -no_deduplicate*)
        ;;
        *)
        ARGS=$ARGS" "$1
        ;;
    esac
    shift
done

#echo esp-clang/bin/ld.lld $ARGS
#esp-clang/bin/ld.lld $ARGS
echo ~/.espressif/tools/riscv32-esp-elf/esp-2022r1-11.2.0/riscv32-esp-elf/bin/riscv32-esp-elf-ld $ARGS
~/.espressif/tools/riscv32-esp-elf/esp-2022r1-11.2.0/riscv32-esp-elf/bin/riscv32-esp-elf-ld $ARGS
exit $?
