#!/bin/bash

ARGS=""
while [[ $# > 0 ]] ; do
    case $1 in
        -arch_only*)
        shift ;;
        -D*)
        ;;
        -o*)
        ;;
        -L*)
        ;;
        -static*)
        ;;
        -syslibroot*)
        shift ;;
        -filelist*)
        while read -r line; do
            ARGS=$line" "$ARGS
        done < $2
        shift ;;
        *)
        ARGS=$1" "$ARGS
        ;;
    esac
    shift
done

#echo esp-clang/bin/llvm-ar $ARGS
#esp-clang/bin/llvm-ar r $ARGS
echo esp-clang/riscv32-esp-elf/bin/ar $ARGS
esp-clang/riscv32-esp-elf/bin/ar r $ARGS
exit $?
